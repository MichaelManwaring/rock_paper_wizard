New Wizard
<%= tag :br %>
<%= form_for @wizard do |w|%>
<%= w.label :Name %>
<%= w.text_field :name %>
<%= tag :br %>
<%= w.label :Magic_Type %>
<%= w.radio_button :magic_type, 0, :checked => true %> Dark Wizard
<%= w.radio_button :magic_type, 1 %> Light Wizard
<%= tag :br %>

<%= w.submit :create, :value => "Create Wizard" %>
<% end %>


<%= link_to "Signout", log_out_path %>